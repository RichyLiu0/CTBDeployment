@using Webdiyer.WebControls.Mvc;
@using DeploymentDAO;
@using Ucsmy.Framework.DAL;

@model Ucsmy.Framework.PagerResponse<D_Project>


@{
    var PageIndex = "pageindex".ValueOfQuery().TryInt(1).Value;
    var PageSize = "pageSize".ValueOfQuery().TryInt(10).Value;
    
}

<html>

<head>
    <title></title>

</head>
<body>

    <table class="table table-striped table-bordered">
        <tr>
            <th class="nowrap">ID
            </th>
            <th class="nowrap">项目名称
            </th>
            <th class="nowrap">项目编号
            </th>
            <th class="nowrap">备注
            </th>
            <th class="nowrap">创建时间
            </th>
            <th class="nowrap">创建人
            </th>

        </tr>


        @foreach (var item in Model.ResultList)
        {

            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ProjectID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ProjectName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ProjectCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Remark)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreateTime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreateUser)
                </td>
            </tr>
        }
    </table>
    @Html.Pager(Model.ResultList.Count, PageSize, PageIndex, new PagerOptions() { CssClass = "color:red;" })
</body>
</html>
